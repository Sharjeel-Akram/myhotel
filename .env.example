# QloApps Environment Configuration
# Copy this file to .env and update the values

# Database Configuration
DB_SERVER=localhost
DB_PORT=3306
DB_NAME=qloapps_db
DB_USER=root
DB_PASSWD=

# Application Configuration
PS_DOMAIN=localhost
PS_HANDLE_DYNAMIC_DOMAIN=0

# Cookie Configuration
PS_COOKIE_KEY=your_cookie_key_here
PS_COOKIE_IV=your_cookie_iv_here

# Development/Production Mode
PS_DEV_MODE=0
PS_DEBUG_MODE=0

# Cache Configuration
PS_SMARTY_CACHE=1
PS_SMARTY_CACHING_TYPE=filesystem

# Security
PS_CIPHER_ALGORITHM=1
_RIJNDAEL_KEY_=your_rijndael_key_here
_RIJNDAEL_IV_=your_rijndael_iv_here
